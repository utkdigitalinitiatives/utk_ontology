@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix utk: <https://ontology.lib.utk.edu/roles#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .

utk:rcp a rdf:Property ;
    skos:prefLabel "Recipient" ;
    rdfs:subPropertyOf dc:contributor ;
    skos:definition "A person, family, or organization to whom the correspondence in a work is addressed"@en ;
    skos:closeMatch "http://id.loc.gov/vocabulary/relators/rcp" ;
    rdfs:range rdfs:Literal;
    rdfs:isDefinedBy utk: .

utk:arc a rdf:Property ;
    skos:prefLabel "Architect" ;
    rdfs:subPropertyOf dc:contributor ;
    skos:definition "A person, family, or organization responsible for creating an architectural design, including a pictorial representation intended to show how a building, etc., will look when completed. It also oversees the construction of structures"@en ;
    skos:closeMatch "http://id.loc.gov/vocabulary/relators/arc" ;
    rdfs:range rdfs:Literal;
    rdfs:isDefinedBy utk: .

utk:arr a rdf:Property ;
    skos:prefLabel "Arranger" ;
    rdfs:subPropertyOf dc:contributor ;
    skos:definition "A person, family, or organization contributing to a musical work by rewriting the composition for a medium of performance different from that for which the work was originally intended, or modifying the work for the same medium of performance, etc., such that the musical substance of the original composition remains essentially unchanged. For extensive modification that effectively results in the creation of a new musical work, see composer"@en ;
    skos:closeMatch "http://id.loc.gov/vocabulary/relators/arr" ;
    rdfs:range rdfs:Literal;
    rdfs:isDefinedBy utk: .

utk:art a rdf:Property ;
    skos:prefLabel "Artist" ;
    rdfs:subPropertyOf dc:contributor ;
    skos:definition "A person, family, or organization responsible for creating a work by conceiving, and implementing, an original graphic design, drawing, painting, etc. For book illustrators, prefer Illustrator [ill]"@en ;
    skos:closeMatch "http://id.loc.gov/vocabulary/relators/art" ;
    rdfs:range rdfs:Literal;
    rdfs:isDefinedBy utk: .

utk:asn a rdf:Property ;
    skos:prefLabel "Associated name" ;
    rdfs:subPropertyOf dc:contributor ;
    skos:definition "A person or organization associated with or found in an item or collection, which cannot be determined to be that of a Former owner [fmo] or other designated relationship indicative of provenance"@en ;
    skos:closeMatch "http://id.loc.gov/vocabulary/relators/asn" ;
    rdfs:range rdfs:Literal;
    rdfs:isDefinedBy utk: .

utk:att a rdf:Property ;
    skos:prefLabel "Attributed name" ;
    rdfs:subPropertyOf dc:contributor ;
    skos:definition "An author, artist, etc., relating him/her to a resource for which there is or once was substantial authority for designating that person as author, creator, etc. of the work"@en ;
    skos:closeMatch "http://id.loc.gov/vocabulary/relators/att" ;
    rdfs:range rdfs:Literal;
    rdfs:isDefinedBy utk: .

utk:aut a rdf:Property ;
    skos:prefLabel "Author" ;
    rdfs:subPropertyOf dc:contributor ;
    skos:definition "A person, family, or organization responsible for creating a work that is primarily textual in content, regardless of media type (e.g., printed text, spoken word, electronic text, tactile text) or genre (e.g., poems, novels, screenplays, blogs). Use also for persons, etc., creating a new work by paraphrasing, rewriting, or adapting works by another creator such that the modification has substantially changed the nature and content of the original or changed the medium of expression"@en ;    skos:closeMatch "http://id.loc.gov/vocabulary/relators/aut" ;
    rdfs:range rdfs:Literal;
    rdfs:isDefinedBy utk: .

utk:ato a rdf:Property ;
    skos:prefLabel "Autographer" ;
    rdfs:subPropertyOf dc:contributor ;
    skos:definition "A person whose manuscript signature appears on an item"@en ;
    skos:closeMatch "http://id.loc.gov/vocabulary/relators/ato" ;
    rdfs:range rdfs:Literal;
    rdfs:isDefinedBy utk: .

utk:bdd a rdf:Property ;
    skos:prefLabel "Binding designer" ;
    rdfs:subPropertyOf dc:contributor ;
    skos:definition "A person or organization responsible for the binding design of a book, including the type of binding, the type of materials used, and any decorative aspects of the binding"@en ;
    skos:closeMatch "http://id.loc.gov/vocabulary/relators/bdd" ;
    rdfs:range rdfs:Literal;
    rdfs:isDefinedBy utk: .

utk:ctg a rdf:Property ;
    skos:prefLabel "Cartographer" ;
    rdfs:subPropertyOf dc:contributor ;
    skos:definition "A person, family, or organization responsible for creating a map, atlas, globe, or other cartographic work"@en ;
    skos:closeMatch "http://id.loc.gov/vocabulary/relators/ctg" ;
    rdfs:range rdfs:Literal;
    rdfs:isDefinedBy utk: .

utk:cns a rdf:Property ;
    skos:prefLabel "Censor" ;
    rdfs:subPropertyOf dc:contributor ;
    skos:definition "A person or organization who examines bibliographic resources for the purpose of suppressing parts deemed objectionable on moral, political, military, or other grounds"@en ;
    skos:closeMatch "http://id.loc.gov/vocabulary/relators/cns" ;
    rdfs:range rdfs:Literal;
    rdfs:isDefinedBy utk: .

utk:chr a rdf:Property ;
    skos:prefLabel "Choreographer" ;
    rdfs:subPropertyOf dc:contributor ;
    skos:definition "A person responsible for creating or contributing to a work of movement"@en ;
    skos:closeMatch "http://id.loc.gov/vocabulary/relators/chr" ;
    rdfs:range rdfs:Literal;
    rdfs:isDefinedBy utk: .

utk: dcterms:modified "2022-08-20"^^xsd:date ;
    dcterms:publisher <http://www.lib.utk.edu/> ;
    dcterms:title "University of Tennessee Digital Initiatives Role Terms Ontology"@en ;
    rdfs:comment """
    Ontology for the the University of Tennessee Libraries to state relationships between people and works.  This ontology
    exists because m3 profiles defined by Houndstooth cannot support both URIs and strings.  For that reason, this ontology
    exists and closely follows marcrelators.  The key difference is that this ontology is defined to work with values
    that are strings only."""@en ;
    rdfs:seeAlso <https://github.com/utkdigitalinitiatives/utk_ontology/> ;
    owl:versionInfo "2022/08/20" .